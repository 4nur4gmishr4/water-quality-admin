# Environment Configuration for Water Quality Monitoring Admin Dashboard

# React App Configuration
REACT_APP_NAME="Water Quality Monitoring Admin"
REACT_APP_VERSION="1.0.0"
REACT_APP_ENVIRONMENT="development"

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY="your_firebase_api_key_here"
REACT_APP_FIREBASE_AUTH_DOMAIN="your_firebase_auth_domain_here"
REACT_APP_FIREBASE_PROJECT_ID="your_firebase_project_id_here"
REACT_APP_FIREBASE_STORAGE_BUCKET="your_firebase_storage_bucket_here"
REACT_APP_FIREBASE_MESSAGING_SENDER_ID="your_firebase_messaging_sender_id_here"
REACT_APP_FIREBASE_APP_ID="your_firebase_app_id_here"

# Supabase Configuration
REACT_APP_SUPABASE_URL="your_supabase_url_here"
REACT_APP_SUPABASE_ANON_KEY="your_supabase_anon_key_here"

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY="your_google_maps_api_key_here"

# ML Model API Configuration
REACT_APP_ML_API_ENDPOINT="http://localhost:5000/api/v1"
REACT_APP_ML_API_KEY="your_ml_api_key_here"

# Government API Configuration (if applicable)
REACT_APP_GOV_API_ENDPOINT="your_government_api_endpoint_here"
REACT_APP_GOV_API_KEY="your_government_api_key_here"

# Feature Flags
REACT_APP_ENABLE_REALTIME="true"
REACT_APP_ENABLE_ML_PREDICTIONS="true"
REACT_APP_ENABLE_MOBILE_SYNC="true"
REACT_APP_ENABLE_NOTIFICATIONS="true"
REACT_APP_ENABLE_ADVANCED_ANALYTICS="true"

# Security Configuration
REACT_APP_ENABLE_HTTPS_ONLY="true"
REACT_APP_SESSION_TIMEOUT_MINUTES="60"
REACT_APP_MAX_LOGIN_ATTEMPTS="5"

# Debug Configuration
REACT_APP_DEBUG_MODE="false"
REACT_APP_LOG_LEVEL="info"

# External Services
REACT_APP_WEATHER_API_KEY="your_weather_api_key_here"
REACT_APP_GEOCODING_API_KEY="your_geocoding_api_key_here"

# Email Configuration (for notifications)
REACT_APP_EMAIL_SERVICE_API_KEY="your_email_service_api_key_here"

# SMS Configuration (for alerts)
REACT_APP_SMS_SERVICE_API_KEY="your_sms_service_api_key_here"

# Analytics Configuration
REACT_APP_ANALYTICS_ID="your_analytics_id_here"

# Default Map Configuration
REACT_APP_DEFAULT_MAP_CENTER_LAT="26.1445"
REACT_APP_DEFAULT_MAP_CENTER_LNG="91.7362"
REACT_APP_DEFAULT_MAP_ZOOM="10"

# Data Refresh Intervals (in seconds)
REACT_APP_DASHBOARD_REFRESH_INTERVAL="30"
REACT_APP_MAP_REFRESH_INTERVAL="60"
REACT_APP_ALERTS_REFRESH_INTERVAL="15"

# UX4G Theme Configuration
REACT_APP_THEME_PRIMARY_COLOR="#1F4E79"
REACT_APP_THEME_SECONDARY_COLOR="#C5282F"
REACT_APP_THEME_SUCCESS_COLOR="#4CAF50"
REACT_APP_THEME_WARNING_COLOR="#FF9800"
REACT_APP_THEME_ERROR_COLOR="#F44336"

# Government Compliance
REACT_APP_COMPLIANCE_LEVEL="GOVERNMENT"
REACT_APP_ACCESSIBILITY_LEVEL="WCAG_2.1_AA"
REACT_APP_DATA_RETENTION_DAYS="2555" # 7 years as per government requirement

# Performance Configuration
REACT_APP_MAX_CONCURRENT_REQUESTS="10"
REACT_APP_REQUEST_TIMEOUT_MS="30000"
REACT_APP_CACHE_DURATION_MINUTES="5"

# Mobile App Integration
REACT_APP_MOBILE_APP_VERSION_MIN="1.0.0"
REACT_APP_MOBILE_SYNC_BATCH_SIZE="50"
REACT_APP_MOBILE_SYNC_INTERVAL_MINUTES="15"
